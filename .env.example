# Clerk Authentication
# Get these from https://dashboard.clerk.com
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# Neon Postgres Database
# Automatically set by Vercel when you create a Neon database
# Or get from Neon dashboard: https://console.neon.tech
POSTGRES_URL=postgresql://user:password@host/dbname?sslmode=require
POSTGRES_PRISMA_URL=postgresql://user:password@host/dbname?connect_timeout=15&sslmode=require
POSTGRES_URL_NON_POOLING=postgresql://user:password@host/dbname?sslmode=require
POSTGRES_USER=user
POSTGRES_HOST=host
POSTGRES_PASSWORD=password
POSTGRES_DATABASE=dbname

# Google Analytics (Optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Yandex Cloud S3 (for match data)
NEXT_PUBLIC_S3_BUCKET_URL=https://storage.yandexcloud.net/your-bucket
